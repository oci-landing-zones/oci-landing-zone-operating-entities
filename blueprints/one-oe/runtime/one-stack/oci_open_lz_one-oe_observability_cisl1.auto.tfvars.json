    "alarms_configuration": {
        "default_compartment_id"               : "CMP-LZP-NETWORK-KEY",

        "alarms": {
            "AL-LZP-NETWORK-LB-KEY": {
                "display_name"                 : "al-lzp-lb-unhealthy-backend",
                "is_enabled"                   : "false",

                "destination_topic_ids": [
                    "NOTT-LZP-NETWORK-ALARMS-KEY"
                ],

                "supplied_alarm": {
                    "message_format"           : "PRETTY_JSON",
                    "namespace"                : "oci_lbaas",
                    "pending_duration"         : "PT5M",
                    "query"                    : "UnHealthyBackendServers[1m].mean() >= 0"
                }
            },

            "AL-LZP-P-NETWORK-VNIC-KEY": {
                "display_name"                 : "al-lzp-p-vnic-drops-alarm",
                "compartment_id"               : "CMP-LZP-P-NETWORK-KEY",
                "is_enabled"                   : "false",

                "destination_topic_ids": [
                    "NOTT-LZP-P-NETWORK-ALARMS-KEY"
                ],

                "supplied_alarm": {
                    "message_format"           : "PRETTY_JSON",
                    "namespace"                : "oci_vcn",
                    "pending_duration"         : "PT5M",
                    "query"                    : "VnicEgressDropsSecurityList[1m].mean() >= 0"
                }
            },

            "AL-LZP-PP-NETWORK-VNIC-KEY": {
                "display_name"                 : "al-lzp-pp-vnic-drops-alarm",
